        "dist/custom-imgmail-design/*"
      ],
      "custom-imgmail-runtime": [
        "dist/custom-imgmail-runtime"
      ],
      "custom-imgmail-runtime/*": [
        "dist/custom-imgmail-runtime/*"
      ],
      "imgsol-custom-mailbox-common": [
        "dist/imgsol-custom-mailbox-common"
      ],
      "imgsol-custom-mailbox-common/*": [
        "dist/imgsol-custom-mailbox-common/*"
      ],
      "imgsol-custom-mailbox-design": [
        "dist/imgsol-custom-mailbox-design"
      ],
      "imgsol-custom-mailbox-design/*": [
        "dist/imgsol-custom-mailbox-design/*"
      ],
      "imgsol-custom-mailbox-runtime": [
        "dist/imgsol-custom-mailbox-runtime"
      ],
      "imgsol-custom-mailbox-runtime/*": [
        "dist/imgsol-custom-mailbox-runtime/*"
      ],
      "test-common": [
        "dist/test-common"
      ],
      "test-common/*": [
        "dist/test-common/*"
      ],
      "test-design": [
        "dist/test-design"
      ],
      "test-design/*": [
        "dist/test-design/*"
      ],
      "test-runtime": [
        "dist/test-runtime"
      ],
      "test-runtime/*": [
        "dist/test-runtime/*"
      ],
      "sample-common": [
        "dist/sample-common"
      ],
      "sample-common/*": [
        "dist/sample-common/*"

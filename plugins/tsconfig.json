        "dist/minimal-common/*"
      ],
      "minimal-design": [
        "dist/minimal-design"
      ],
      "minimal-design/*": [
        "dist/minimal-design/*"
      ],
      "minimal-runtime": [
        "dist/minimal-runtime"
      ],
      "minimal-runtime/*": [
        "dist/minimal-runtime/*"
      ]
